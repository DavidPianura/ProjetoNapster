package napster;

import java.io.File;
import java.util.ArrayList;

public class NapsterMessage {
	private String request;
	private static ArrayList<String> properties;
	
	public NapsterMessage(String request, ArrayList<String> properties) {
		this.request = request;
		this.properties = properties;
	}
	
	public ArrayList<String> getProperties(String folder) {
		return this.properties;
	}
	
}
